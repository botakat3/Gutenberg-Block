{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "kb/project-block",
  "version": "0.1.0",
  "title": "Project Block",
  "category": "design",
  "icon": "project-block",
  "description": "Add your projects!",
  "keywords": [
    "project"
  ],
  "example": {},
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "textdomain": "high-pulp-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "project": {
      "type": "string",
      "source": "text",
      "selector": ".project"
    },
    "overview": {
      "type": "string",
      "source": "html",
      "selector": ".overview"
    },
    "buttonUrl": {
      "type": "string",
      "source": "attribute",
      "selector": ".button",
      "attribute": "href"
    },
    "imageUrl": {
      "type": "string",
      "default": "https://placehold.co/75"
    }
  }
}